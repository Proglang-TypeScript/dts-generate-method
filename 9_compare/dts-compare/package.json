{
  "name": "dts-compare",
  "version": "1.0.0",
  "description": "Compares 2 parsed typescript declaration files",
  "bin": {
    "dts-compare": "dist/index.js",
    "dts-parse": "dist/parse.js"
  },
  "scripts": {
    "compare": "npm run build && dts-compare",
    "parse": "npm run build && dts-parse",
    "prebuild": "rm -rf dist",
    "build": "npx tsc",
    "postbuild": "npm link",
    "test": "jest"
  },
  "author": "fcristiani",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.7.0",
    "jest": "^25.2.3",
    "ts-jest": "^25.2.1"
  }
}
